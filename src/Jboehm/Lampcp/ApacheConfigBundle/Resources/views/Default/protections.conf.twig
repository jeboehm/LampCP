{% for protection in protections %}
<Directory "{{ protection.fullpath }}">
    AuthType Basic
    AuthName "{{ protection.realm }}"
    AuthUserFile {{ protection.domain.path }}/conf/authuser_{{ protection.id }}.passwd
    require valid-user
</Directory>

{% endfor %}
