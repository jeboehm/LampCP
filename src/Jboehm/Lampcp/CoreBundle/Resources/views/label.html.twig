{% block field_label %}
    {% spaceless %}
        <label for="{{ id }}">
            {% if label is not null %}
                {{ label|trans() }}
            {% else %}
                {{ id|replace({'_':'.'})|trans() }}
            {% endif %}
        </label>
    {% endspaceless %}
{% endblock field_label %}