<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8"/>
    <title>{% block headtitle %}Welcome!{% endblock %}</title>
    {% block stylesheets %}
        <link rel="stylesheet" type="text/css" href="{{ asset("bootstrap/css/bootstrap.css") }}"/>
        <link rel="stylesheet" type="text/css" href="{{ asset("bootstrap/css/bootstrap-responsive.css") }}"/>
    {% endblock %}
</head>
<body>
{% block body %}{% endblock %}
{% block javascripts %}
    <script src="{{ asset("bootstrap/js/jquery-1.9.1.js") }}" type="text/javascript"></script>
    <script src="{{ asset("bootstrap/js/bootstrap.min.js") }}" type="text/javascript"></script>
{% endblock %}
</body>
</html>
